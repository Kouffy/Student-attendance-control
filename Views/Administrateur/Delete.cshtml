@model MiniProjet_alpha.Model.Administrateur

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Supprimer</h1>

<h3>Confirmer</h3>
<div>
    <h4>Gestion des administrateurs</h4>
    <hr />
     <dl class="row">
   <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdAdministrateur)
        </dt>
        <dd class = "col-sm-10">
